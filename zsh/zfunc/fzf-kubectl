fzf-kubectl::print() {
    if ! which kubectl 1>/dev/null 2>&1; then
        echo 'A "kubectl" command is not found.'
        return 1
    fi
    local s=$(kubectl get all -o name | fzf)
    [[ -n $s ]] && print "$s"
}

[[ "x$BUFFER" != "x" ]] && BUFFER="$BUFFER "
BUFFER="$BUFFER$(fzf-kubectl::print)"
CURSOR=$#BUFFER
zle redisplay

